{
    "name": "Weather Application",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "Shows you the weather",
    "homepage_url": "https://abhiprasad.github.io",
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "default_locale": "en",
    "background": {
        "page": "src/bg/background.html",
        "persistent": true
    },
    "browser_action": {
        "default_icon": "icons/icon19.png",
        "default_title": "Weather",
        "default_popup": "src/browser_action/index.html"
    }
}